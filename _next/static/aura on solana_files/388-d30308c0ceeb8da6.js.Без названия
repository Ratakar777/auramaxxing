			}`,fragmentShader:`
				uniform sampler2D mirrorSampler;
				uniform float alpha;
				uniform float time;
				uniform float size;
				uniform float distortionScale;
				uniform sampler2D normalSampler;
				uniform vec3 sunColor;
				uniform vec3 sunDirection;
				uniform vec3 eye;
				uniform vec3 waterColor;

				varying vec4 mirrorCoord;
				varying vec4 worldPosition;

				vec4 getNoise( vec2 uv ) {
					vec2 uv0 = ( uv / 103.0 ) + vec2(time / 17.0, time / 29.0);
					vec2 uv1 = uv / 107.0-vec2( time / -19.0, time / 31.0 );
					vec2 uv2 = uv / vec2( 8907.0, 9803.0 ) + vec2( time / 101.0, time / 97.0 );
					vec2 uv3 = uv / vec2( 1091.0, 1027.0 ) - vec2( time / 109.0, time / -113.0 );
					vec4 noise = texture2D( normalSampler, uv0 ) +
						texture2D( normalSampler, uv1 ) +
						texture2D( normalSampler, uv2 ) +
						texture2D( normalSampler, uv3 );
					return noise * 0.5 - 1.0;
				}

				void sunLight( const vec3 surfaceNormal, const vec3 eyeDirection, float shiny, float spec, float diffuse, inout vec3 diffuseColor, inout vec3 specularColor ) {
					vec3 reflection = normalize( reflect( -sunDirection, surfaceNormal ) );
					float direction = max( 0.0, dot( eyeDirection, reflection ) );
					specularColor += pow( direction, shiny ) * sunColor * spec;
					diffuseColor += max( dot( sunDirection, surfaceNormal ), 0.0 ) * sunColor * diffuse;
				}

				#include <common>
				#include <packing>
				#include <bsdfs>
				#include <fog_pars_fragment>
				#include <logdepthbuf_pars_fragment>
				#include <lights_pars_begin>
				#include <shadowmap_pars_fragment>
				#include <shadowmask_pars_fragment>

				void main() {

					#include <logdepthbuf_fragment>
					vec4 noise = getNoise( worldPosition.xz * size );
					vec3 surfaceNormal = normalize( noise.xzy * vec3( 1.5, 1.0, 1.5 ) );

					vec3 diffuseLight = vec3(0.0);
					vec3 specularLight = vec3(0.0);

					vec3 worldToEye = eye-worldPosition.xyz;
					vec3 eyeDirection = normalize( worldToEye );
					sunLight( surfaceNormal, eyeDirection, 100.0, 2.0, 0.5, diffuseLight, specularLight );

					float distance = length(worldToEye);

					vec2 distortion = surfaceNormal.xz * ( 0.001 + 1.0 / distance ) * distortionScale;
					vec3 reflectionSample = vec3( texture2D( mirrorSampler, mirrorCoord.xy / mirrorCoord.w + distortion ) );

					float theta = max( dot( eyeDirection, surfaceNormal ), 0.0 );
					float rf0 = 0.3;
					float reflectance = rf0 + ( 1.0 - rf0 ) * pow( ( 1.0 - theta ), 5.0 );
					vec3 scatter = max( 0.0, dot( surfaceNormal, eyeDirection ) ) * waterColor;
					vec3 albedo = mix( ( sunColor * diffuseLight * 0.3 + scatter ) * getShadowMask(), ( vec3( 0.1 ) + reflectionSample * 0.9 + reflectionSample * specularLight ), reflectance);
					vec3 outgoingLight = albedo;
					gl_FragColor = vec4( outgoingLight, alpha );

					#include <tonemapping_fragment>
					#include <${parseInt(r.REVISION.replace(/\D+/g,""))>=154?"colorspace_fragment":"encodings_fragment"}>
					#include <fog_fragment>	
				}`},R=new r.ShaderMaterial({fragmentShader:A.fragmentShader,vertexShader:A.vertexShader,uniforms:r.UniformsUtils.clone(A.uniforms),lights:!0,side:m,fog:v});R.uniforms.mirrorSampler.value=M.texture,R.uniforms.textureMatrix.value=T,R.uniforms.alpha.value=l,R.uniforms.time.value=s,R.uniforms.normalSampler.value=u,R.uniforms.sunColor.value=d,R.uniforms.waterColor.value=f,R.uniforms.sunDirection.value=c,R.uniforms.distortionScale.value=p,R.uniforms.eye.value=h,n.material=R,n.onBeforeRender=function(e,t,r){if(b.setFromMatrixPosition(n.matrixWorld),w.setFromMatrixPosition(r.matrixWorld),x.extractRotation(n.matrixWorld),y.set(0,0,1),y.applyMatrix4(x),E.subVectors(b,w),E.dot(y)>0)return;E.reflect(y).negate(),E.add(b),x.extractRotation(r.matrixWorld),S.set(0,0,-1),S.applyMatrix4(x),S.add(w),C.subVectors(b,S),C.reflect(y).negate(),C.add(b),_.position.copy(E),_.up.set(0,1,0),_.up.applyMatrix4(x),_.up.reflect(y),_.lookAt(C),_.far=r.far,_.updateMatrixWorld(),_.projectionMatrix.copy(r.projectionMatrix),T.set(.5,0,0,.5,0,.5,0,.5,0,0,.5,.5,0,0,0,1),T.multiply(_.projectionMatrix),T.multiply(_.matrixWorldInverse),g.setFromNormalAndCoplanarPoint(y,b),g.applyMatrix4(_.matrixWorldInverse),P.set(g.normal.x,g.normal.y,g.normal.z,g.constant);let i=_.projectionMatrix;k.x=(Math.sign(P.x)+i.elements[8])/i.elements[0],k.y=(Math.sign(P.y)+i.elements[9])/i.elements[5],k.z=-1,k.w=(1+i.elements[10])/i.elements[14],P.multiplyScalar(2/P.dot(k)),i.elements[2]=P.x,i.elements[6]=P.y,i.elements[10]=P.z+1-a,i.elements[14]=P.w,h.setFromMatrixPosition(r.matrixWorld);let o=e.getRenderTarget(),l=e.xr.enabled,s=e.shadowMap.autoUpdate;n.visible=!1,e.xr.enabled=!1,e.shadowMap.autoUpdate=!1,e.setRenderTarget(M),e.state.buffers.depth.setMask(!0),!1===e.autoClear&&e.clear(),e.render(t,_),n.visible=!0,e.xr.enabled=l,e.shadowMap.autoUpdate=s,e.setRenderTarget(o);let u=r.viewport;void 0!==u&&e.state.viewport(u)}}}}}]);